        { "command": "type", "target": "id=password", "value": "staff123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//h1[contains(.,'Staff Dashboard')]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update') or contains(normalize-space(.),'Update Status')])[1]", "value": "" },
        { "command": "click", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update') or contains(normalize-space(.),'Update Status')])[1]", "value": "" },
        { "command": "click", "target": "xpath=(//*[contains(.,'Ready for Pickup') or contains(.,'ready_for_pickup')])[1]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update Status') or contains(normalize-space(.),'Update')])[1]", "value": "" },
        { "command": "click", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update Status') or contains(normalize-space(.),'Update')])[1]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//span[contains(.,'Ready for Pickup') or contains(.,'ready_for_pickup')]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//span[contains(.,'Ready for Pickup') or contains(.,'ready_for_pickup')]", "value": "" }
      ]
    },

    {
      "id": "tc06-admin-modify-schedule",
      "name": "TC06 - Admin modifies floor schedule",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "type", "target": "id=email", "value": "admin@college.edu" },
        { "command": "type", "target": "id=password", "value": "admin123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//h1[contains(.,'Admin Dashboard') or contains(.,'Laundry Schedules')]", "value": "" },
        { "command": "open", "target": "/admin/schedules", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//*[contains(text(),'Laundry Schedules') or contains(text(),'Date-Specific Schedules')]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//*[contains(text(),'Laundry Schedules') or contains(text(),'Date-Specific Schedules')]", "value": "" }
      ]
    },

    {
      "id": "tc07-invalid-form",
      "name": "TC07 - Invalid form submission",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//*[contains(text(),'Please') or contains(text(),'required') or contains(text(),'Invalid credentials') or contains(text(),'Login failed') ]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//*[contains(text(),'Please') or contains(text(),'required') or contains(text(),'Invalid credentials') or contains(text(),'Login failed') ]", "value": "" }
      ]
    },

    {
      "id": "tc08-responsiveness",
      "name": "TC08 - Check dashboard responsiveness",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "type", "target": "id=email", "value": "student123@college.edu" },
        { "command": "type", "target": "id=password", "value": "student123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//h1[contains(.,'Student Dashboard')]", "value": "" },
        { "command": "setWindowSize", "target": "375,812", "value": "" },
        { "command": "pause", "target": "500", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//h1[contains(.,'Student Dashboard')]", "value": "" }
      ]
    }
  ],
  "suites": [
    {
      "id": "all-tests",
      "name": "All Tests",
      "persistSession": false,
      "parallel": false,
      "timeout": 300,
      "tests": [
        "tc01-login-valid",
        "tc02-login-invalid",
        "tc03-student-schedule",
        "tc04-staff-update-status",
        "tc05-notification-ready",
        "tc06-admin-modify-schedule",
        "tc07-invalid-form",
        "tc08-responsiveness"
      ]
    }
  ],
  "urls": ["http://localhost:3000"],
  "plugins": []
