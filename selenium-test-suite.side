{
  "id": "laundry-management-system-tests",
  "version": "2.0",
  "name": "Laundry Management System - TC01-TC08",
  "url": "http://localhost:3000",
  "tests": [
    {
      "id": "tc01-login-valid",
      "name": "TC01 - Verify user login with valid credentials",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "type", "target": "id=email", "value": "john.doe@student.college.edu" },
        { "command": "type", "target": "id=password", "value": "student123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//h1[contains(.,'Student Dashboard')]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//h1[contains(.,'Student Dashboard')]", "value": "" }
      ]
    },

    {
      "id": "tc02-login-invalid",
      "name": "TC02 - Verify invalid login attempt",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "type", "target": "id=email", "value": "abc@vit.in" },
        { "command": "type", "target": "id=password", "value": "wrong" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//*[contains(text(),'Invalid credentials') or contains(text(),'Login failed') or contains(text(),'Please') ]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//*[contains(text(),'Invalid credentials') or contains(text(),'Login failed') or contains(text(),'Please') ]", "value": "" }
      ]
    },

    {
      "id": "tc03-student-schedule",
