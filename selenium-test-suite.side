        { "command": "type", "target": "id=password", "value": "student123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//*[contains(text(),'Next Laundry Day') or contains(.,'Next Laundry Day')]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//*[contains(text(),'Next Laundry Day') or contains(.,'Next Laundry Day')]", "value": "" }
      ]
    },

    {
      "id": "tc04-staff-update-status",
      "name": "TC04 - Update laundry status by staff",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
        { "command": "type", "target": "id=email", "value": "staff@college.edu" },
        { "command": "type", "target": "id=password", "value": "staff123" },
        { "command": "click", "target": "css=button[type=\"submit\"]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//h1[contains(.,'Staff Dashboard')]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update') or contains(normalize-space(.),'Update Status')])[1]", "value": "" },
        { "command": "click", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update') or contains(normalize-space(.),'Update Status')])[1]", "value": "" },
        { "command": "click", "target": "xpath=//div[contains(@class,'select') or contains(@class,'Select')]//div[contains(.,'Washing') or contains(.,'washing')][1]", "value": "" },
        { "command": "type", "target": "css=textarea", "value": "In Wash" },
        { "command": "waitForElementPresent", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update Status') or contains(normalize-space(.),'Update')])[1]", "value": "" },
        { "command": "click", "target": "xpath=(//*[self::button or self::a or self::div][contains(normalize-space(.),'Update Status') or contains(normalize-space(.),'Update')])[1]", "value": "" },
        { "command": "waitForElementPresent", "target": "xpath=//span[contains(.,'Washing') or contains(.,'washing')]", "value": "" },
        { "command": "assertElementPresent", "target": "xpath=//span[contains(.,'Washing') or contains(.,'washing')]", "value": "" }
      ]
    },

    {
      "id": "tc05-notification-ready",
      "name": "TC05 - Notification on laundry ready",
      "commands": [
        { "command": "open", "target": "/", "value": "" },
